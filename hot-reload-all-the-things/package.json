{
  "name": "trying-to-hot-reload-all-the-things",
  "scripts": {
    "start": "rm -rf ./.build && npm-run-all --parallel start:server start:client",
    "start:server": "webpack --config webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "npm-run-all": "^4.0.2",
    "react-hot-loader": "next",
    "start-server-webpack-plugin": "^2.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.3",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
